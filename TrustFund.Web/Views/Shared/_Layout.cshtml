<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Trust Fund</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <link href="~/Content/loading-placeholder.css" rel="stylesheet" media="screen">
    <link href="~/Content/Layout.css" rel="stylesheet" media="screen">
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/themes/base/css")
    @RenderSection("baseTag", false)
</head>
<body>
    @Html.Partial("~/Views/Shared/_TopBar.cshtml")
    <div class="wrapper">
        <div class="container" style="padding-top: 15px;">
            <div class="content-wrapper main-content clear-fix">
                <div style="margin-top: 35px;">
                    <a href="@Url.Action("Index", "Home")">
                    </a>
                </div>
                <section id="mainbody">
                    @RenderBody()
                </section>
            </div>
        </div>
        <div class="push"></div>
    </div>
    @Html.Partial("~/Views/Shared/_BottomBar.cshtml")

    <script type="text/javascript">
        window.TrustFund = {};
        (function (tf) {
            var rootPath;
            tf.rootPath = rootPath;
        }(window.TrustFund));
        TrustFund.rootPath = '@Url.Content("~")';
    </script>

    <script src="~/Scripts/moment.js" type="text/javascript"></script>
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/angular-route.js"></script>
    <script src="~/Scripts/angular-animate.js"></script>

    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/bootstrap")

    <script src="~/Scripts/angular-ui/ui-bootstrap-tpls.js"></script>
    <script src="@Url.Content("~/Scripts/App.js")" type="text/javascript"></script>

    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        @RenderSection("footerScripts", required: false)
    </script>
</body>
</html>
